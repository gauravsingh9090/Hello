class A{
     public static void main(String args[]){
          for(int i=1;i<=100;i++){ 
              if(isprime(i)){
                  System.out.println(i+"us aprime number");
              }
          }
     }
     public static boolean isprime(int num){
         if(num<=1){
             return false ;
             
         }
         for(int i=2;i<=Math.sqrt(num);i++){
             if(num%i==0){
                 return false;
                 
             }
         }
         return true;
     }
}
